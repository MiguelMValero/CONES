startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         0.005;

deltaT          0.005;

writeControl    timeStep;

writeInterval   2;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

cwipiSwitch     true;

cwipiDim        3;

cwipiStep       1;

cwipiObs 	 5;

cwipiMembers 	 10;

cwipiParams	 1;


