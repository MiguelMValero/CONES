6
(
    movingWall
    {
        type            wall;
        inGroups        List<word> 1(wall);
        nFaces          0;
        startFace       85;
    }
    fixedWalls
    {
        type            wall;
        inGroups        List<word> 1(wall);
        nFaces          5;
        startFace       85;
    }
    frontAndBack
    {
        type            empty;
        inGroups        List<word> 1(empty);
        nFaces          100;
        startFace       90;
    }
    procBoundary1to0
    {
        type            processor;
        inGroups        List<word> 1(processor);
        nFaces          10;
        startFace       190;
        matchTolerance  0.0001;
        myProcNo        1;
        neighbProcNo    0;
    }
    procBoundary1to2
    {
        type            processor;
        inGroups        List<word> 1(processor);
        nFaces          10;
        startFace       200;
        matchTolerance  0.0001;
        myProcNo        1;
        neighbProcNo    2;
