FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "40/uniform";
    object      fieldAverage(U,p,prime2Mean=yes)Properties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

U
{
    totalIter       201;
    totalTime       40.2;
}

p
{
    totalIter       201;
    totalTime       40.2;
}


